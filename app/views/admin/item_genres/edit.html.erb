<div class="container">
  <div class="row">
    <div class="col">
      <h4>ジャンル編集</h4>
        <%= form_with model: @item_genre, do |f| %>
          <div class="form">
            <p><b>ジャンル名</b></p>
            <%= f.text_field :name %>
            <%= f.submit '変更を保存', class:"btn btn-success" %>
          </div>
        <% end %>
    </div>
  </div>
</div>