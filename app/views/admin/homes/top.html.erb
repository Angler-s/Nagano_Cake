<div class="container">
  <h3>注文履歴一覧</h3>
  <div>
    <table class="table">
      <thead>
        <tr>
          <th>購入日時</th>
          <th>購入者</th>
          <th>注文個数</th>
          <th>注文ステータス</th>
        </tr>
      </thead>
      <% @orders.each do |customer| %>
      <tbody>
        <tr>
          <td>
            <!--購入日時-->
            <%= link_to l order.created_at, admin_order_path(order) %>
          </td>
            <!--購入者名-->
          <td>
            <%= order.customer.last_name %>
            <%= order.customer.first_name %>
          </td>
            <!--注文個数-->
          <td>

          </td>
            <!--注文ステータス-->
          <td><%= order.status %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>