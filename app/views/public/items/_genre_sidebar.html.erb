<div class="col-md-2 my-5">
  <table class="table table-bordered my-4">
    <thead class="table">
      <tr><th>ジャンル検索</th></tr>
    </thead>
    <tbody class="table table-borderless">
      <% genres.each do |genre| %>
        <tr>
          <td class="color-dark">
            <%= link_to genre.name, item_genre_path(genre) %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>